    <datasources>
        <datasource id="DataSource1" type="sql" openRole="begin" source="static" connection="internal" parentID="" connID="sibcues">select  CONCAT(codigo, '/', anioInventario  ) codigoBien, 
       descripcionClase, fechaAdquisicion, 
       valor, descripcionSector,

unidad.nombreUnidad as unidad,
unidad.idUnidadP1 as P1,
unidad.idUnidadP2 as P2,
unidad.directorUnidad as director,
unidad.jefaturaUnidad as jefatura
        
from bienactivofijo bien 
join especbien espec  on bien.idEspec=espec.idEspec
join sectores sector on bien.idSector=sector.idSector
join invactivofijo invaf on invaf.idInvAF=bien.idInvAF 
join inventario inv on inv.idInventario=invaf.idInventario
LEFT JOIN unidad ON unidad.idUnidad=inv.idUnidad 
where  inv.idUnidad= $P{idUnidad} 
AND anioInventario LIKE '$P{anioInventarioSelecc}' 
AND descripcionSector LIKE '$P{descripcionSectoSelecc}'
order by descripcionSector, descripcionClase, fechaAdquisicion;</datasource>
    </datasources>
